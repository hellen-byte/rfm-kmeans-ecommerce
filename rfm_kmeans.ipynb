# ==========================================
# RFM + CLUSTERIZAÃ‡ÃƒO DE CLIENTES (K-MEANS)
# ==========================================

import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
from sklearn.preprocessing import StandardScaler
from sklearn.cluster import KMeans

# 1. Base RFM a partir de df_vendas
df_rfm = df_vendas.copy()

# Data de referÃªncia para recÃªncia (Ãºltima data de pedido)
data_ref = df_rfm["data_pedido"].max()

# 2. CÃ¡lculo de RecÃªncia, FrequÃªncia e Valor por cliente
df_rfm_calc = (
    df_rfm
    .groupby("cliente_id")
    .agg({
        "data_pedido": lambda x: (data_ref - x.max()).days,  # recÃªncia: dias desde a Ãºltima compra
        "id_pedido": "count",                                # frequÃªncia: nÂº de pedidos
        "receita_liquida": "sum"                             # valor: quanto esse cliente jÃ¡ gastou
    })
    .reset_index()
)

df_rfm_calc.columns = ["cliente_id", "recencia", "frequencia", "valor"]

# 3. CriaÃ§Ã£o dos scores R, F, M (de 1 a 5)
# Quanto MENOR a recÃªncia, MAIOR a nota â†’ por isso rÃ³tulos invertidos
df_rfm_calc["R_score"] = pd.qcut(df_rfm_calc["recencia"], 5, labels=[5,4,3,2,1])

# FrequÃªncia e Valor: quanto MAIOR, MAIOR a nota
df_rfm_calc["F_score"] = pd.qcut(
    df_rfm_calc["frequencia"].rank(method="first"),
    5, labels=[1,2,3,4,5]
)
df_rfm_calc["M_score"] = pd.qcut(
    df_rfm_calc["valor"],
    5, labels=[1,2,3,4,5]
)

# Score RFM concatenado
df_rfm_calc["RFM_score"] = (
    df_rfm_calc["R_score"].astype(str) +
    df_rfm_calc["F_score"].astype(str) +
    df_rfm_calc["M_score"].astype(str)
)

# 4. SegmentaÃ§Ã£o textual a partir do score RFM
def segmentar(rfm):
    if rfm in ["555", "554", "545", "544", "455", "454", "445", "444"]:
        return "TOP (CampeÃµes)"
    elif rfm[0] in ["4", "5"]:
        return "Leais"
    elif rfm[1] in ["4", "5"]:
        return "Frequentes"
    elif rfm[2] in ["4", "5"]:
        return "Alto Valor"
    elif rfm[0] == "3":
        return "Potenciais"
    elif rfm[0] == "2":
        return "AtenÃ§Ã£o"
    else:
        return "Em risco"

df_rfm_calc["segmento"] = df_rfm_calc["RFM_score"].apply(segmentar)

print("PrÃ©via da tabela RFM:")
print(df_rfm_calc.head())

# -----------------------------
# 5. CLUSTERIZAÃ‡ÃƒO (K-MEANS)
# -----------------------------

# Features numÃ©ricas para cluster
X = df_rfm_calc[["recencia", "frequencia", "valor"]]

# PadronizaÃ§Ã£o (mÃ©dia 0, desvio 1) â€“ importantÃ­ssimo pro K-Means
scaler = StandardScaler()
X_scaled = scaler.fit_transform(X)

# 5.1 MÃ©todo do cotovelo para escolher k
sse = []
ks = range(1, 10)
for k in ks:
    km = KMeans(n_clusters=k, random_state=42)
    km.fit(X_scaled)
    sse.append(km.inertia_)

plt.figure(figsize=(7,4))
plt.plot(ks, sse, marker="o")
plt.title("MÃ©todo do Cotovelo")
plt.xlabel("NÃºmero de clusters (k)")
plt.ylabel("SSE (InÃ©rcia)")
plt.show()

# Escolha do k (visualmente o "joelho" costuma ser 3 ou 4)
k_escolhido = 4  # se quiser testar 3 depois, Ã© sÃ³ trocar aqui

kmeans = KMeans(n_clusters=k_escolhido, random_state=42)
df_rfm_calc["cluster"] = kmeans.fit_predict(X_scaled)

print("\nDistribuiÃ§Ã£o de clientes por cluster:")
print(df_rfm_calc["cluster"].value_counts())

# 5.2 VisualizaÃ§Ã£o simples dos clusters (RecÃªncia x Valor)
plt.figure(figsize=(8,4))
plt.scatter(
    df_rfm_calc["recencia"],
    df_rfm_calc["valor"],
    c=df_rfm_calc["cluster"]
)
plt.xlabel("RecÃªncia (dias desde a Ãºltima compra)")
plt.ylabel("Valor (receita acumulada do cliente)")
plt.title("Clusters de Clientes (RecÃªncia x Valor)")
plt.show()

# 5.3 Perfil mÃ©dio de cada cluster
cluster_profile = (
    df_rfm_calc
    .groupby("cluster")[["recencia", "frequencia", "valor"]]
    .mean()
    .round(2)
)

print("\nPerfil mÃ©dio de cada cluster:")
print(cluster_profile)

# 5.4 Cruzar cluster com segmento RFM
print("\nCruzamento Segmento RFM x Cluster:")
print(pd.crosstab(df_rfm_calc["cluster"], df_rfm_calc["segmento"]))



import pandas as pd
import numpy as np
import seaborn as sns
import matplotlib.pyplot as plt
from scipy import stats

# ==============================
# 1. Selecionar variÃ¡vel de interesse (receita lÃ­quida)
# ==============================

variavel = "receita_liquida"

print(f"Analisando outliers da variÃ¡vel: {variavel}")
print(df_vendas[variavel].describe())

# ==============================
# 2. Histograma + KDE
# ==============================

plt.figure(figsize=(7,4))
sns.histplot(df_vendas[variavel], kde=True)
plt.title(f"DistribuiÃ§Ã£o de {variavel}")
plt.xlabel(variavel)
plt.ylabel("FrequÃªncia")
plt.show()

# ==============================
# 3. Boxplot
# ==============================

plt.figure(figsize=(7,3))
sns.boxplot(x=df_vendas[variavel])
plt.title(f"Boxplot de {variavel}")
plt.show()

# ==============================
# 4. MÃ©todo IQR (Interquartile Range)
# ==============================

Q1 = df_vendas[variavel].quantile(0.25)
Q3 = df_vendas[variavel].quantile(0.75)
IQR = Q3 - Q1

limite_inferior = Q1 - 1.5 * IQR
limite_superior = Q3 + 1.5 * IQR

outliers_iqr = df_vendas[
    (df_vendas[variavel] < limite_inferior) | 
    (df_vendas[variavel] > limite_superior)
]

print("\n--- OUTLIERS PELO MÃ‰TODO IQR ---")
print(f"Total de outliers: {len(outliers_iqr)}")
print(outliers_iqr[[variavel]].head())

# ==============================
# 5. MÃ©todo Z-SCORE
# ==============================

df_vendas["zscore"] = stats.zscore(df_vendas[variavel])

outliers_z = df_vendas[df_vendas["zscore"].abs() > 3]

print("\n--- OUTLIERS PELO MÃ‰TODO Z-SCORE ---")
print(f"Total de outliers: {len(outliers_z)}")
print(outliers_z[[variavel, "zscore"]].head())

# ==============================
# 6. ComparaÃ§Ã£o final
# ==============================

print("\nComparaÃ§Ã£o de quantidades:")
print(f"IQR: {len(outliers_iqr)} outliers")
print(f"Z-score: {len(outliers_z)} outliers")

# ==============================
# 7. InterpretaÃ§Ã£o automÃ¡tica (EXTRA)
# ==============================

media = df_vendas[variavel].mean()
maximo = df_vendas[variavel].max()

print("\n--- INTERPRETAÃ‡ÃƒO AUTOMÃTICA ---")
if maximo > media * 5:
    print("âš ï¸ Existem vendas MUITO acima da mÃ©dia â€” provÃ¡vel cluster de clientes premium.")
elif len(outliers_iqr) == 0:
    print("Nenhum outlier relevante â€” distribuiÃ§Ã£o limpa.")
else:
    print("Existem valores acima do esperado, podem representar picos de venda ou produtos de ticket alto.")

# Remover zscore temporÃ¡rio
df_vendas.drop(columns=["zscore"], inplace=True)


